(executable
 (name test_fq)
 (modules test_fq)
 (libraries bls12_381 alcotest))

(executable
 (name test_fr)
 (modules test_fr)
 (libraries bls12_381 alcotest))


(alias
 (name    runtest-bls12-381-test-fq)
 (action  (run %{exe:test_fq.exe})))

(alias
 (name    runtest-bls12-381-test-fr)
 (action  (run %{exe:test_fr.exe})))


(alias
 (name    runtest)
 (deps    (alias runtest-bls12-381-test-fq) (alias runtest-bls12-381-test-fr)))
